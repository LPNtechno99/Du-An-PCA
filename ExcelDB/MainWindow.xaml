<Window x:Class="ExcelDB.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ExcelDB"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <StackPanel>
            <Button Content="ShowData" Click="showData_buttonClick" />
            <Button Content="UpdateData" Click="updateData_Click" />
            <DataGrid Name="dbMain" CellEditEnding="dbMain_CellEditEnding">

            </DataGrid>
        </StackPanel>
        
    </Grid>
</Window>
